<div class="container">
  <div class="logo">
    <span class="large">S</span>EQUENCE
  </div>
</div>
<div class="container registration">
  <div class="row">
    <div class="col-md-6 col-sm-12 sign-up-container">
      <%= devise_error_messages! %>
      <%= form_for(resource, :html => {:class => "form-horizontal sign-up-form"}, as: resource_name, url: registration_path(resource_name)) do |f| %>
      <div class="form-group">
        <!-- <label class="col-sm-12 col-md-3 control-label">Email</label> -->
        <%= f.label :email, :class => ('col-sm-12 col-md-3 control-label') %>
        <div class="col-sm-12 col-md-8">
          <!-- <input type="email" class="form-control" placeholder="input email" name="email"> -->
          <%= f.email_field :email, :class => ('form-control'), :placeholder => ('email'), autofocus: true %>
        </div>
      </div>
      <div class="form-group">
        <!-- <label class="col-sm-12 col-md-3 control-label">Username</label> -->
        <%= f.label :username, :class => ('col-sm-12 col-md-3 control-label') %>
        <div class="col-sm-12 col-md-8">
          <!-- <input type="text" class="form-control" placeholder="username" name="username"> -->
          <%= f.text_field :username, :class => ('form-control'), :placeholder => ('username'), autocomplete: "off"%>
        </div>
      </div>
      <div class="form-group">
        <!-- <label class="col-sm-12 col-md-3 control-label">Password</label> -->
        <%= f.label :password, :class => ('col-sm-12 col-md-3 control-label') %>
        <div class="col-sm-12 col-md-8">
          <!-- <input type="password" class="form-control" placeholder="password" name="password"> -->
          <%= f.password_field :password, :class => ('form-control'), :placeholder => ('password'), autocomplete: "off" %>
        </div>
      </div>
      <div class="form-group">
        <!-- <label class="col-sm-12 col-md-3 control-label">Confirm password</label> -->
        <%= f.label :Confirm_password, :class => ('col-sm-12 col-md-3 control-label') %>
        <div class="col-sm-12 col-md-8">
          <!-- <input type="Password" class="form-control" placeholder="confirm password" name="password_confirmation"> -->
          <%= f.password_field :password_confirmation, :class => ('form-control'), :placeholder => ('confirm password'), autocomplete: "off" %>
        </div>
      </div>
      <p class="avatar-instruction text-primary">Select an avatar</p>
      <div class="row avatar-select">
